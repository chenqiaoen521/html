page{width:100%;height:100%; background: #f7f7f7}
.coupon{display: -ms-flexbox;display: -webkit-box;display: flex; height:115px;width: 90%;margin:19rpx auto 38rpx; border-radius:7px;border:1px dashed #dcdcdc; }
.coupon .left{-ms-flex:0 0 70%;-webkit-box-flex:0;flex:0 0 70%;text-align: center;border-right:1px dashed #dcdcdc;}
.coupon .up {background: #f7f7f7;height: 90px;}
.coupon .down {font-size: 12px; color: #999999;line-height: 25px;}
.coupon .left .up{font-size: 13px;color: #ff6698;border-radius: 7px 0 0 0;padding-top: 18px;}
.coupon .left .up .price{font-size: 30px;}
.coupon .left .up .price-desc{color: #b9b9b9;font-size: 12px;}
.coupon .right{-ms-flex:1;-webkit-box-flex:1;flex:1;position: relative;}
.coupon .right .up{overflow: hidden; border-radius: 0 7px 0 0; background: #f8b347; display: -webkit-box; display: -ms-flexbox; display: flex; -webkit-box-orient: vertical; -webkit-box-direction: normal; -ms-flex-direction: column; flex-direction: column; -webkit-box-pack: justify; -ms-flex-pack: justify; justify-content: space-between; padding-bottom: 20rpx;}
.coupon .right .name{font-size: 13px; color: #ff6698;font-weight: normal;margin-top: 55rpx;padding:3rpx 5rpx;margin:0 auto;text-align: center; border-radius:20rpx;border:2px solid #ed8015;   width: 170rpx; background: #fff;}
.coupon .right .desc{font-size: 12px; color: #fff;padding:5rpx 10rpx; text-align: center;margin-top: 40rpx;}
.coupon .right .down{padding-left: 35rpx;text-align: center;}

.disable {color: #b9b9b9 !important;}
.disable .down{color: #b9b9b9 !important;}
.disable .left .up{color: #b9b9b9 !important;}
.disable .right .name{color: #b9b9b9 !important;}
