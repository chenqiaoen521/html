.cardlist{ padding: 30rpx;}
.cardlist .sg{ border: 2rpx dashed #ff6698; margin-bottom: 20rpx; background-color: #fff; border-radius: 10rpx; font-size: 28rpx; overflow: hidden;}
.cardlist .sg .sgright{ float:left; width: 45%; padding: 40rpx 20rpx; }
.cardlist .sg .sgmid{ float:left; width: 20%; padding: 40rpx 0; text-align: center; border-left: 1px dashed #ccc; border-right: 1px dotted #ccc;  background-color: rgba(255,102,152,0.1) }
.cardlist .sg .sgleft{ float:left; width: 35%; padding: 40rpx 0; text-align: center; }
.cardlist .sg .sgprice{ display: block; font-size: 30rpx; color: #444;}
.cardlist .sg .sgred{ font-size: 48rpx; padding-left: 6rpx;}
.cardlist .sg .mid{ font-size: 30rpx; color: #333; text-align: center; display: block; margin-bottom: 20rpx;}
.cardlist .sg .sgbtn{ width: 60%; display: block; background-color: #ff6698; color: #fff; font-size: 28rpx; text-align: center; border-radius: 4px; line-height: 60rpx; margin: 0 auto; margin-bottom: 16rpx;}
.cardlist .sg .sgtime{ font-size: 24rpx; color: #777; line-height:40rpx;}
.cardlist .sg .sgtime .timetip{ font-weight: bold; }
.cardlist .sg .sgcolor{ color: #ff6698;}

.cardlist .sg:nth-child(7n+1){ border: 2rpx dashed #f53648;}
.cardlist .sg:nth-child(7n+1) .sgcolor{  color: #f53648;}
.cardlist .sg:nth-child(7n+1) .sgbtn{ background-color: #f53648;}
.cardlist .sg:nth-child(7n+1) .sgmid{ background-color: rgba(245,54,72,0.1)}

