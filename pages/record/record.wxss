image{display: block;}
page{width:100%;height:100%; background: #f4f4f4;}
.scroll{padding-bottom: 80rpx;}
.cell{background: #ffffff;border-bottom: 2rpx solid #e5e5e5;border-top: 2rpx solid #e5e5e5;margin-bottom: 20rpx;}
.cell .product{display: -webkit-box;display: -ms-flexbox;display: flex;padding:20rpx 30rpx;border-bottom: 1rpx solid #e5e5e5;}

.cell .product .img{width:165rpx; height: 165rpx;}
.cell .product .desc{-webkit-box-flex: 1;-ms-flex: 1;flex: 1;padding:0 25rpx;}
.cell .product .desc .name{font-size: 32rpx; color: #333333;line-height: 46rpx;display: -webkit-box;
-webkit-box-orient: vertical;
-webkit-line-clamp: 2;overflow:hidden;}
.cell .product .desc .score{font-size: 30rpx; color: #999999;padding-top: 30rpx;}
.cell .product .desc .score>text{color: #ff2020;}
.cell .bottom{height: 110rpx;padding:0 20rpx;}
.cell .bottom .date{line-height: 110rpx;font-size: 32rpx;color: #999999;}
.cell .bottom .btn{float: right;border-radius: 9rpx;  margin-top: 20rpx;line-height:70rpx;padding:0 20rpx;  border: 1rpx solid #ff6698;font-size: 30rpx;color: #ff6698;   background-color: #fff; }
.none>image{width: 200rpx;height: 230rpx;margin: 300rpx auto 0;text-align: center;}
.none .txt{text-align: center;margin-top: 30rpx;font-size: 34rpx;color: #030000;}