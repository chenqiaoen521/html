.mask{position: fixed;left: 0;right: 0;bottom:1000px; padding-bottom:200px;color: #fff; z-index:-5; background:#fff ; box-shadow: 0 0 5px #eee;}
.mask .do{ overflow: hidden; height: 72rpx; line-height: 72rpx; background-color: #f7f7f7; padding: 0 30rpx; font-size: 28rpx; color: #666;}
.mask .do .cancle{ display: block; float: left;}
.mask .do .surea{ display: block; float: right; color: #349dde;}
.pickers{ position: absolute; left: 0; bottom: 0; width: 100%; height: 200px;}
.picker-view{text-align: center;line-height: 34px !important; color: #444; font-size: 28rpx; height: 34px !important;}
.none{ opacity: 0;}
.display{opacity: 1; bottom:0; z-index: 35;/*position: static;*/}