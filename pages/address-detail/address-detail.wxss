page{width:100%;height:100%; background: #f4f4f4;}
.address{position: relative; height: 92rpx;background: #fff;line-height:92rpx; border-bottom: 1px solid #e8e8e8;}
.address .dd{float:left;margin-left: 20rpx;font-size:30rpx;font-weight:bold;}
.address .left{height:92rpx;line-height: 92rpx; padding:0 55rpx 0 200rpx;}
.address .left input{height:92rpx;line-height: 92rpx;color:#888;font-size:28rpx;text-align:right;}
.address .left .input {font-size:15px;color:#323232;height:92rpx;line-height: 92rpx;color:#888;font-size:28rpx;text-align:right;}
.address .title {height:92rpx;line-height: 92rpx;color:#888;font-size:28rpx;text-align:right;}
.address .left .picker{line-height: 30px;font-size: 15px; color: #323232;}
.address .desc{font-size: 12px;color: #888888; }
.address .right{position: absolute; top:50%; right: 0; margin-top:-7px;  display: block;width: 7px;height: 13px;margin-right: 20rpx; }
.default {height:92rpx;line-height: 92rpx;}
.address.default .switch {float: right;line-height: 92rpx;font-size: 0;margin-right: 20rpx;}
.complete{position: absolute;left: 0;right: 0; bottom: 0; height: 44px;line-height: 44px; font-size: 16px;text-align: center; color: #fff;  background: #ff6698;}
.clearfix:after{clear: both;content: '';}
