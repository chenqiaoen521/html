/*确认订单 开始*/
page{ background-color: #f0f1f5; padding-bottom: 92rpx;  }
.make{padding: 0;}
.make .place{ padding: 12rpx 80rpx 12rpx 90rpx; position: relative; border-bottom: 2rpx solid #ddd; margin-bottom: 12rpx; background-color: #fff; border-top: 2rpx solid #ddd;}
.make .place .wz{ display: block; width: 28rpx; height: 42rpx; position: absolute; left: 28rpx; top: 50%; margin-top: -21rpx; z-index: 33; background-color: f00; }
.make .place .cimg{ display: block; width: 21rpx; height: 35rpx; position: absolute; right: 20rpx; top: 50%; margin-top: -18rpx; z-index: 33; }
.make .place .person{ padding: 0; overflow: hidden;} 
.make .place .person .pn{ display: block; float: left; font-size: 30rpx; color: #222; line-height: 60rpx;}
.make .place .person .pp{ display: block; float: right; font-size: 26rpx; color: #444; line-height: 60rpx;}
.make .place .td{ font-size: 26rpx; color: #777; line-height: 40rpx; display: block;}
.adda{ padding: 10rpx 30rpx; font-size: 28rpx; line-height: 64rpx; color: #3ea3e9; }

.make .shop{ padding: 0; margin-bottom: 12rpx; }
.make .shop .h2{ border-top: 2rpx solid #ddd; border-bottom: 2rpx solid #dedede; padding: 0 20rpx; font-size: 30rpx; color: #222; line-height: 80rpx; background-color: #fff;}
.make .shop .list{ background-color: #f9f9f9;}
.make .shop .list .sg{ display: block; position: relative; padding: 24rpx 80rpx 24rpx 20rpx; height: 250rpx; border-bottom: 2rpx solid #ddd;}
.make .shop .list .sg .tno{ display: block; position: absolute; right: 0; bottom: 20rpx; width: 50rpx; color: #555; font-size: 28rpx; height: 40rpx; line-height: 40rpx; }
.make .shop .list .sg navigator{ display: block; padding-left:220rpx; position: relative; padding-top: 14rpx; } 
.make .shop .list .sg navigator image{ display: block; border: 2rpx solid #eee; width: 200rpx; height: 200rpx; position: absolute; left: 0; top: 0; border-radius: 6rpx; box-sizing: border-box;}
.make .shop .list .sg navigator .tn{ display: block; font-size: 28rpx; color: #222; line-height: 40rpx; max-height: 80rpx; overflow: hidden;}
.make .shop .list .sg navigator .td{ display: block; font-size: 26rpx; color: #888; line-height: 40rpx; margin: 10rpx 0;}
.make .shop .list .sg navigator .tm{ display: block; font-size: 28rpx; color: #ff6698; font-weight: bold; line-height: 40rpx;}

.make .set{ padding: 0 0 60rpx 0;}
.make .set .dl{ padding: 10rpx 20rpx; background-color: #f7f7f7; border-bottom: 2rpx solid #eee; position: relative; line-height: 72rpx; color: #222; font-size: 30rpx;}
.make .set .dl .dt{ display: block; height: 72rpx; }
.make .set .dl .dd{ display: block; line-height: 72rpx; text-align: right; }
.make .set .dl .dd input{ display: block; border: none; height: 72rpx; line-height: 72rpx; color: #888; font-size: 28rpx; text-align: right;}
.make .set .dl .dd .picker{ display: block; height: 72rpx; line-height: 72rpx; color: #888; font-size: 28rpx; text-align: right; }
.make .set .dl .dd navigator{ display: block; height: 72rpx; line-height: 72rpx; color: #ff6698; font-size: 28rpx; text-align: right; }
.make .set .dl .dd .hong{ display: block; height: 72rpx; line-height: 72rpx; color: #ff6698; font-size: 28rpx; text-align: right; }
.make .set .dl .dd navigator.navigator-hover { color:#ff6698; background-color: transparent; opacity: 1;}
.make .set .dl .h6{ display: block; font-size: 25rpx; color: #3ea3e9; line-height: 60rpx; }
.make .set .dl .h3{ display: block; font-size: 28rpx; color: #777; line-height: 60rpx; text-align: right; }
.make .set .dl .inline{ color: #ff6698; padding: 0 4rpx;}
.make .set .dl .jt{ display: block; width:16rpx; height: 27rpx; position: absolute; right: 20rpx; top:50%; margin-top: -14rpx;}
.make .set .dl textarea{ display: block; border: none; min-height: 120rpx; padding:10rpx; color: #888; font-size: 28rpx; width: 100%; position: static;}
.make .set .dl>input{color: #888; font-size: 28rpx; width: 100%; height: auto; display: block; border: none; padding:18rpx 10rpx; }
.make .set .dl.on{ padding: 10rpx 50rpx 10rpx 200rpx; background-color: #fff;}
.make .set .dl.on .dt{ position: absolute; left: 20rpx; }
.make .set .dl.on.blue{background: #f7f7f7;border:none;padding-left: 30rpx;}
.make .set .dl .inlineb{ color: #3ea3e9; padding: 0 4rpx;}
.make .set .dl .chose{ width: 36rpx; height: 36rpx; margin: 18rpx 0; float: right;}
.make .set .dl .chose .con{ width: 36rpx; height: 36rpx; display: none; }
.make .set .dl .chose .con.on{ display: block;}


/*确认订单 结束*/
button{ border-radius: 0;}
.cancle{ display: block; position: fixed; width: 50%; height: 92rpx; color: #fff; background-color: #666; left: 0; bottom: 0; font-size: 30rpx; line-height: 92rpx; text-align: center; z-index: 3333;}
.sure{ display: block; position: fixed; width: 100%; height: 92rpx; color: #fff; background-color: #ff6698; right: 0; bottom: 0; font-size: 30rpx; line-height: 92rpx; text-align: center; z-index: 3333;}

/** 修改默认的navigator点击态 **/
.navigator-hover { color:inherit; background-color: transparent; opacity: 1;}
/** 自定义其他点击态样式类 **/
.other-navigator-hover { color:inherit; background-color: transparent; opacity: 1;}