/*确认订单 开始*/
page{ background-color: #f0f1f5; padding-bottom: 92rpx;  }
.make{padding: 0;}
.make .set{ padding: 16rpx 0 60rpx 0;}
.make .set .dl{ padding: 10rpx 20rpx; border-top: 2rpx solid #ddd; border-bottom: 2rpx solid #eee; position: relative; line-height: 72rpx; color: #222; font-size: 30rpx; margin-bottom: 16rpx; overflow: hidden;}
.make .set .dl .dt{ display: block; height: 72rpx; overflow: hidden; }
.make .set .dl .dd{ display: block; line-height: 72rpx; text-align: right; }
.make .set .dl .tn{ display: block; font-size: 30rpx; color: #777; line-height: 52rpx;}
.make .set .dl .td{ display: block; font-size: 30rpx; color: #ff6698; line-height: 52rpx;}
.make .set .h3{ display: block; font-size: 30rpx; color: #777; line-height: 84rpx; padding: 0rpx 20rpx 0 20rpx; }
.make .set .dl .inline{ color: #ff6698; padding: 0 4rpx;}
.make .set .dl .dd .hong{ color: #ff6698;}
.make .set .dl .dd .lan{ color: #3ea3e9;}
.make .set .dl .inline text{ font-size: 36rpx; padding-left: 4rpx;}
.make .set .dl .jt{ display: block; width:16rpx; height: 27rpx; position: absolute; right: 20rpx; top:50%; margin-top: -14rpx;}
.make .set .dl textarea{ display: block; border: none; min-height: 120rpx; padding:10rpx; color: #888; font-size: 28rpx; width: 100%;}
.make .set .dl.on{ padding: 10rpx 20rpx 10rpx 200rpx; background-color: #fff;}
.make .set .dl.on.blue{background: #f7f7f7;border:none;padding-left: 30rpx;}
.make .set .dl .inlineb{ color: #3ea3e9; padding: 0 4rpx;}
.make .set .dl.on .dt{ position: absolute; left: 20rpx; }
.make .set .dl .wcon{ display: block; float: left; width: 48rpx; height: 48rpx; margin: 12rpx 16rpx 12rpx 0; }
.make .set .dl .dt text{ display: block; float: left; line-height: 72rpx;}
.make .set .dl .chose{ width: 36rpx; height: 36rpx; margin: 18rpx 0; float: right;}
.make .set .dl .chose .con{ width: 36rpx; height: 36rpx; display: none; }
.make .set .dl .chose .con.on{ display: block;}


/*确认订单 结束*/
button{ border-radius: 0;}
.cancle{ display: block; position: fixed; width: 50%; height: 92rpx; color: #fff; background-color: #666; left: 0; bottom: 0; font-size: 30rpx; line-height: 92rpx; text-align: center; z-index: 3333;}
.sure{ display: block; position: fixed; width: 100%; height: 92rpx; color: #fff; background-color: #ff6698; right: 0; bottom: 0; font-size: 30rpx; line-height: 92rpx; text-align: center; z-index: 3333;}

/** 修改默认的navigator点击态 **/
.navigator-hover { color:#fff; background-color: #ff6698; opacity: 1;}
/** 自定义其他点击态样式类 **/
.other-navigator-hover { color:#fff; background-color: #ff6698; opacity: 1;}