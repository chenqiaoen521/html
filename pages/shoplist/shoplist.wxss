/*搜索*/
.findn{ position: fixed; left: 0; top: 0; width: 100%; z-index: 33; background-color: #fff;}
.findn .find{padding:12rpx 20rpx; background-color: #f5f5f5; box-sizing: border-box; }
.findn .fa{ display: block; width: 100%; height: 72rpx; line-height: 72rpx; font-size: 30rpx; color: #555; padding: 0 92rpx 0 20rpx; position: relative; background-color: #fff; border-radius:36rpx;}
.findn .fa input{ display: block; height: 72rpx; border: none; font-size: 28rpx; line-height: 72rpx; color: #555;}
.findn .fa text{ display: block; height: 72rpx; border: none; font-size: 28rpx; line-height: 72rpx; color: #555;}
.findn .fa image{ display: block; width: 72rpx; height: 72rpx; position: absolute; right: 16rpx; top: 0; padding: 14rpx; box-sizing: border-box;}
/*搜索*/

/*筛选 开始*/
.findn .kids{ border-bottom: 2rpx solid #eee; overflow: hidden;}
.findn .kids .one{ display: block; width: 33.33%; float: left; text-align: center; font-size: 28rpx; line-height: 82rpx; border-bottom: 4rpx solid #fff; overflow: hidden;}
.findn .kids .one view{ display: inline-block; height: 82rpx; padding-right: 30rpx; position: relative; /*background-position: right center; background-repeat: no-repeat; background-size: 20rpx auto; background-image: url("../img/shoplist_13.png");*/}
.findn .kids .one view text{ display: block;}
.findn .kids .one view image{ position: absolute; right: 0; top: 50%; margin-top: -13rpx; width:20rpx; height: 26rpx; display: none; }
.findn .kids .one view image.on{ display: block;}
.findn .kids .one.bu view{ padding-right: 0;}
.findn .kids .one.on{ border-bottom: 4rpx solid #ff6698;}
.findn .tedian{ border-bottom: 2rpx solid #eee; overflow: hidden;  }
.findn .tedian .two{ display: block; width: 33.33%; float: left; text-align: center; font-size: 26rpx; line-height:76rpx;}
.findn .tedian .two view{ display: inline-block; padding-right: 30rpx; background-position: right center; background-repeat: no-repeat; background-size: 20rpx auto; background-image: url("../img/shoplist_24.png");}
.findn .tedian .two.on view{ background-image: url("../img/shoplist_22.png");}
/*筛选 开始*/



/*分类列表*/
page{ padding-top: 185rpx;}
.kinds{ border-bottom: 16rpx solid #ededed;}
.ksts{ display:flex; background-color: #FFFFFF; flex-direction:row;width:100%;-webkit-flex-wrap:wrap;flex-wrap:wrap;}
.ksts .sin{width:50%;}
.ksts .sin .sg{ padding:14rpx; border-bottom: 2rpx solid #eee; border-right: 2rpx solid #eee; }
.ksts .sin .simg{display:block;background-color: #eeeeee;width:100%; height: 0; position: relative; padding-bottom: 100%;}
.ksts .sin .simg image{ display: block; width: 100%; height: 100%; position: absolute; left: 0; top: 0; }
.ksts .sin .sname{font-size:26rpx; margin:8rpx 10rpx; overflow: hidden;text-overflow: ellipsis; display: block; white-space: nowrap; line-height: 48rpx;}
.ksts .sin .sprice{ margin:10rpx 10rpx; overflow: hidden;text-overflow: ellipsis; display: block; white-space: nowrap; text-align:left; font-size:24rpx; color: #3cc51f; height: 36rpx; line-height:36rpx;}
.ksts .sin .sprice .smoney{ float: left; color: #ff6698; font-size:28rpx; }
.ksts .sin .sprice .snum{ float: right; color: #888;}
.ksts .sin .sprice .big{ font-size:36rpx;}
.ksts .sin .sprice .color{ color: #ff6698;}
.ksts .sin:nth-child(2n) .sg{ border-right: 2rpx solid #fff;}
/*分类列表*/

/**筛选条件 开始**/
.popbg{ position: fixed; left: 0; top: 264rpx; width: 100%; height: 100%; background-color: rgba(0,0,0,0.5); z-index: 32; display: none;}
.popkind{ position: fixed; left: 0; top: 264rpx; width: 100%; z-index: 33; background-color: #fff; display: none;}
.popkind .same{ width: 100%; overflow: hidden; overflow-y: auto; }
.popkind .same .group{ display:block; overflow: hidden; padding: 30rpx 20rpx; }
.popkind .same .group text{ width: 48%; display: block; float: left; line-height:72rpx; height: 72rpx; color: #444; text-align: center; font-size: 28rpx; margin: 0 1%;}
.popkind .same .group text.on{ color: #fff; background-color: #ff6698;}
.popkind .btn{ display: block; overflow: hidden;}
.popkind .btn .ann{ display: block; height: 92rpx; line-height: 92rpx; color: #444; font-size: 30rpx; text-align: center; background-color: #ededed; width: 50%; float: left;}
.popkind .btn .ann.sure{ background-color: #ff6698; color:#fff;}

.popbg.on{ display: block;}
.popkind.on{ display: block;}

/**筛选条件 开始**/



.body {width:234rpx;margin: 0 auto;}
.body image{width:234rpx; height: 212.94rpx;margin-top:79.56rpx;}
.body .text{margin: 23px auto;font-size: 15px; color: #030000;width: 100%; text-align: center;}
.body .jump{font-size: 15px; color: #ff6698;line-height: 30px; text-align: center; height: 30px;border: 1rpx solid #ff6698;border-radius: 10px; display: block;}