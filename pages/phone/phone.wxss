page{width:100%;height:100%; background: #f4f4f4}
.item{display: flex; background: #fff;height: 45px; line-height: 45px;padding: 0 32rpx;border-bottom: 1rpx solid #e5e5e5;color: #333333; font-size: 13px;}
.item .left{flex:0 0 110rpx;text-align: left;}
.item .right{flex:1;line-height:45px;}
.item .right .input{height: 45px;vertical-align:bottom;display: inline-block;}
.item .right .btn{background: #f2f2f2;margin-top: 5px;padding:0 30rpx; border-radius: 5px;font-size: 12px; color: #999999;float: right;line-height: 35px;}
.title{height: 40px;line-height: 40px; font-size: 13px; color: #666666;padding: 0 20rpx;}


.message {font-size: 11px; color: #494949; text-align: center;margin-top: 35rpx;line-height: 38rpx;}
.message .phone {font-size: 13px; color: #494949; }

.complete{position: absolute;left: 0;right: 0; bottom: 0; height: 44px;line-height: 44px; font-size: 16px;text-align: center; color: #fff;  background: #ff6698;}